<div class="blockwrapper">
{{#each blocks as |block|}}
    <div class="wrapper blockbox">
    {{#each block as |cell|}}
        {{#if cell.generated}}
            <div class="box">{{cell.symbol}}</div>
        {{else}}
            {{#selectable-field symbols=symbols cell=cell}}{{/selectable-field}}
        {{/if}}
    {{/each}}
    </div>
{{/each}}
</div>
{{#if solved}}
    <div>Gelöst!</div>
{{else}}
    <button class="btn" {{action "check"}} disabled={{not model.dirty}}>Prüfen</button> Leider noch nicht gelöst.
{{/if}}
